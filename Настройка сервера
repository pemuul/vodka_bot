server {
        listen 80;
        #listen [::]:80;
        server_name designer-tg-bot.ru www.designer-tg-bot.ru;
        underscores_in_headers on;
        root /home/server/;


        location /static {
                alias /home/server /static;
        }

        location / {
                proxy_pass http://localhost:8000;
                include /etc/nginx/proxy_params;
                proxy_redirect off;
        }
}
server {
        listen 80;
        server_name 80.78.243.187;
        return 301 http://designer-tg-bot.ru;
}
server {
        listen 443 ssl http2;
        #listen [::]:443 ssl;
        server_name designer-tg-bot.ru www.designer-tg-bot.ru;
        underscores_in_headers on;
        root /home/server/;

        ssl_protocols       TLSv1 TLSv1.1 TLSv1.2;
        ssl_ciphers         HIGH:!aNULL:!MD5;

        #ssl on;
        ssl_certificate /home/ssl/designer-tg-bot.ru.crt; # managed by Certbot
        ssl_certificate_key /home/ssl/designer-tg-bot.ru.key;

        location /static {
                alias /home/server/static;
        }

        location / {
                proxy_pass http://localhost:8000;
                include /etc/nginx/proxy_params;
                proxy_redirect off;
        }
}




